
# Composer binary
# Not included in project but could be present for some users
composer.phar

# Composer manages Wordpress core, plugins and vendor libs
/wordpress/
/app/plugins/*
/app/uploads/
/vendor/

# Some plugins are not handled by Composer
!/app/plugins/sitepress-multilingual-cms/
!/app/plugins/wpml-string-translation/
!/app/plugins/wp-tinyletter-widget

# Wordpress server details should never be included in version control
.htaccess
wp-config.php

# Generated resources are ignored
main.min.css
scripts.min.js
